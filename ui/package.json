{
	"name": "ui",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "npm run prepare-theme && vite dev",
		"build": "npm run prepare-theme && vite build",
		"preview": "vite preview",
		"prepare": "npm run prepare-theme && svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"prepare-theme": "npm run smui-theme-light && npm run smui-theme-dark",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
		"smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@material/typography": "^14.0.0",
		"@mdi/js": "^7.4.47",
		"@smui-extra/accordion": "^8.0.0-beta.3",
		"@smui-extra/autocomplete": "^8.0.0-beta.3",
		"@smui/button": "^8.0.0-beta.3",
		"@smui/card": "^8.0.0-beta.3",
		"@smui/checkbox": "^8.0.0-beta.3",
		"@smui/circular-progress": "^8.0.0-beta.3",
		"@smui/common": "^8.0.0-beta.3",
		"@smui/drawer": "^8.0.0-beta.3",
		"@smui/fab": "^8.0.0-beta.3",
		"@smui/form-field": "^8.0.0-beta.3",
		"@smui/icon-button": "^8.0.0-beta.3",
		"@smui/layout-grid": "^8.0.0-beta.3",
		"@smui/linear-progress": "^8.0.0-beta.3",
		"@smui/list": "^8.0.0-beta.3",
		"@smui/menu": "^8.0.0-beta.3",
		"@smui/paper": "^8.0.0-beta.3",
		"@smui/radio": "^8.0.0-beta.3",
		"@smui/select": "^8.0.0-beta.3",
		"@smui/slider": "^8.0.0-beta.3",
		"@smui/tab": "^8.0.0-beta.3",
		"@smui/tab-bar": "^8.0.0-beta.3",
		"@smui/textfield": "^8.0.0-beta.3",
		"@smui/tooltip": "^8.0.0-beta.3",
		"@smui/top-app-bar": "^8.0.0-beta.3",
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"@tailwindcss/vite": "^4.0.0",
		"@tanstack/svelte-query": "^5.76.0",
		"@tanstack/svelte-query-devtools": "^5.76.0",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"flowbite": "^3.1.2",
		"flowbite-svelte": "^1.4.3",
		"flowbite-svelte-icons": "^2.2.0",
		"globals": "^16.0.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"smui-theme": "^8.0.0-beta.3",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.0.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^6.2.6"
	},
	"dependencies": {
		"@sveltejs/adapter-static": "^3.0.8",
		"@threlte/core": "^8.0.4",
		"@types/three": "^0.176.0",
		"three": "^0.176.0"
	}
}
